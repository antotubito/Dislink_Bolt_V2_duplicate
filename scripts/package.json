{
    "name": "dislink-database-backup",
    "version": "1.0.0",
    "description": "Automated Supabase database backup and restore system",
    "main": "database-backup.ts",
    "scripts": {
        "backup:create": "tsx database-backup.ts backup",
        "backup:restore": "tsx database-backup.ts restore",
        "backup:list": "tsx database-backup.ts list",
        "backup:cleanup": "tsx database-backup.ts cleanup",
        "backup:schedule": "tsx database-backup.ts schedule",
        "backup:restore:confirm": "tsx database-backup.ts restore",
        "backup:restore:dry": "tsx database-backup.ts restore --dry-run",
        "build": "tsc database-backup.ts --outDir dist",
        "start": "node dist/database-backup.js"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.39.3",
        "node-cron": "^3.0.3",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/node-cron": "^3.0.11",
        "tsx": "^4.6.0",
        "typescript": "^5.2.2"
    },
    "keywords": [
        "supabase",
        "database",
        "backup",
        "restore",
        "automation",
        "scheduling"
    ],
    "author": "Dislink Team",
    "license": "MIT"
}